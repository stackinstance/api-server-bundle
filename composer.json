{
    "name" : "stackinstance/api-server-bundle",
    "description" : "Stack Instance Api Server",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Ray Kootstra",
        "email" : "r.kootstra@stackinstance.com"
    }],
    "keywords" : [
        "api server bundle"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.5.0",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "phpunit/phpunit": "4.8.*",
        "doctrine/doctrine-migrations-bundle": "1.0.*",
        "doctrine/migrations": "~1.0"
    },
    "repositories" : [{
        "type" : "vcs",
        "url" : "https://github.com/stackinstance/api-server-bundle"
    }],
    "autoload": {
        "psr-0": { "StackInstance\\ApiServerBundle": "" }
    },

    "target-dir": "StackInstance/ApiServerBundle",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true
}
